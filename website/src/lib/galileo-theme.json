{
  "name": "galileo-deep-ocean",
  "displayName": "Galileo Deep Ocean",
  "type": "dark",
  "colors": {
    "editor.background": "#0A0A12",
    "editor.foreground": "#E5E5E5",
    "editor.lineHighlightBackground": "#0D0D1A",
    "editor.selectionBackground": "#00FFFF33",
    "editorCursor.foreground": "#00FFFF",
    "editorWhitespace.foreground": "#2A2A3A",
    "editorLineNumber.foreground": "#4A4A5A",
    "editorLineNumber.activeForeground": "#6B6B7B"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#4A4A5A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.other",
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#00FF88"
      }
    },
    {
      "name": "String escape characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#00CC6A"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#4488FF"
      }
    },
    {
      "name": "Function parameters",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#E5E5E5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types and Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "entity.other.inherited-class",
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "#D4AF37"
      }
    },
    {
      "name": "Type parameters",
      "scope": [
        "meta.type.parameters",
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#D4AF37"
      }
    },
    {
      "name": "Interfaces",
      "scope": [
        "entity.name.type.interface"
      ],
      "settings": {
        "foreground": "#D4AF37"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex"
      ],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#E5E5E5"
      }
    },
    {
      "name": "Object properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#E5E5E5"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.separator",
        "punctuation.terminator",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#6B6B7B"
      }
    },
    {
      "name": "Decorators and Annotations",
      "scope": [
        "meta.decorator",
        "meta.decorator entity.name.function"
      ],
      "settings": {
        "foreground": "#D4AF37"
      }
    },
    {
      "name": "Tags (JSX/HTML)",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Tag attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#D4AF37",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON property names",
      "scope": [
        "support.type.property-name.json",
        "string.json support.type.property-name"
      ],
      "settings": {
        "foreground": "#4488FF"
      }
    },
    {
      "name": "JSON string values",
      "scope": [
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#00FF88"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "name": "Markdown headings",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#00FFFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#E5E5E5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#E5E5E5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#4488FF"
      }
    },
    {
      "name": "Solidity - address/mapping/uint types",
      "scope": [
        "support.type.primitive.solidity",
        "entity.name.type.solidity"
      ],
      "settings": {
        "foreground": "#D4AF37"
      }
    },
    {
      "name": "Solidity - visibility modifiers",
      "scope": [
        "keyword.control.solidity",
        "storage.modifier.solidity"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "CSS - property names",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name.css"
      ],
      "settings": {
        "foreground": "#4488FF"
      }
    },
    {
      "name": "CSS - property values",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#E5E5E5"
      }
    },
    {
      "name": "CSS - selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.css"
      ],
      "settings": {
        "foreground": "#D4AF37"
      }
    },
    {
      "name": "Template interpolation",
      "scope": [
        "punctuation.definition.template-expression",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Import/export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "this/self",
      "scope": [
        "variable.language.this",
        "variable.language.self"
      ],
      "settings": {
        "foreground": "#FF6B6B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Namespace",
      "scope": [
        "entity.name.namespace",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#E5E5E5"
      }
    },
    {
      "name": "Enum members",
      "scope": [
        "variable.other.enummember",
        "constant.other.enum"
      ],
      "settings": {
        "foreground": "#D4AF37"
      }
    }
  ]
}
